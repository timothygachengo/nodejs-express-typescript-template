{
    "name": "nodejs-express-typescript-starter",
    "version": "1.0.0",
    "main": "server.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "node build/src/server.js",
        "build": "rimraf ./build && gulp config-scripts && gulp build",
        "dev": "nodemon",
        "start:build": "yarn run build && nodemon build/src/server.js",
        "test": "jest",
        "test:watch": "jest --watchAll --detectOpenHandles --coverage --verbose",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write '**/*.{js,json,md}'",
        "format:check": "prettier --check '**/*.{js,json,md}'",
        "migrate:latest": "knex migrate:latest",
        "seed:run": "knex seed:run",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:latest:watch": "knex migrate:latest --watch"
    },
    "_moduleAliases": {
        "@example": "./build/src/",
        "@example/api": "./build/src/api",
        "@example/config": "./build/src/config",
        "@example/core": "./build/src/core",
        "@example/database": "./build/src/database",
        "@example/loaders": "./build/src/loaders",
        "@example/services": "./build/src/services",
        "@example/utils": "./build/src/utils"
    },
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/uuid": "^8.3.3",
        "apollo-server-express": "^3.5.0",
        "axios": "^0.24.0",
        "bcryptjs": "^2.4.3",
        "chalk": "4.1.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dayjs": "^1.10.7",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-jsdoc-swagger": "^1.6.7",
        "express-monitoring": "^1.0.1",
        "express-status-monitor": "^1.2.3",
        "express-validator": "^6.14.0",
        "graphql": "15.3.0",
        "graphql-depth-limit": "^1.1.0",
        "helmet": "^4.6.0",
        "jose": "^4.3.7",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.15",
        "module-alias": "^2.2.2",
        "mongoose": "^6.1.3",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.3",
        "node-emoji": "^1.11.0",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typedi": "^0.10.0",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.5",
        "@babel/plugin-proposal-class-properties": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-status-monitor": "^1.2.4",
        "@types/graphql-depth-limit": "^1.1.3",
        "@types/gulp": "^4.0.9",
        "@types/gulp-copy": "^0.0.4",
        "@types/gulp-sourcemaps": "^0.0.35",
        "@types/jest": "^27.0.3",
        "@types/module-alias": "^2.0.1",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.8",
        "@types/node-emoji": "^1.8.1",
        "@types/shelljs": "^0.8.10",
        "@types/supertest": "^2.0.11",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "babel-cli": "^6.26.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^25.3.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^27.4.5",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "superagent": "^6.1.0",
        "supertest": "^6.1.6",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "ts-jest": "^27.1.2",
        "ts-jest-resolver": "^2.0.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "typescript-module-alias": "^1.0.2"
    },
    "require": [
        "ts-node/register"
    ],
    "engines": {
        "node": ">=14.0.0"
    }
}
